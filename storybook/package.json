{
  "name": "dialtone-vue-storybook",
  "version": "1.0.0",
  "description": "Storybook for Dialtone Vue",
  "scripts": {
    "storybook": "NODE_PATH=./node_modules storybook dev -c ../.storybook --no-version-updates --ci -p 9010",
    "build": "NODE_PATH=./node_modules storybook build -c ../.storybook -o compiled",
    "storybook-a11y-test": "NODE_PATH=./node_modules node ./scripts/storybook-a11y-test.js"
  },
  "devDependencies": {
    "@shopify/storybook-a11y-test": "^1.1.2",
    "@storybook/addon-a11y": "^7.0.5",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-backgrounds": "^7.0.5",
    "@storybook/addon-controls": "^7.0.5",
    "@storybook/addon-docs": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/addon-mdx-gfm": "^7.0.5",
    "@storybook/addon-styling": "^1.0.1",
    "@storybook/addon-viewport": "^7.0.5",
    "@storybook/addons": "^7.0.5",
    "@storybook/manager-api": "^7.0.5",
    "@storybook/source-loader": "^7.0.5",
    "@storybook/theming": "^7.0.5",
    "@storybook/vue": "^7.0.5",
    "@storybook/vue-vite": "^7.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.5",
    "vue-template-compiler": "^2.7.14"
  },
  "dependencies": {
    "vue": "^2.7.14"
  },
  "engineStrict": true,
